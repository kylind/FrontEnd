<h2 class="pageTitle1">{{ Congratulations_ }}</h2>
<p class="tPad1">{{ You_ve_successfully_applied_for_this_job_ }}</p>
{% if APPLIED_JOB_DATA.jobname %}<p class="tPad1"><strong>{{ Position_ }}</strong> <a href="{{ buildJobDetailUrl(PORTAL_URL, APPLIED_JOB_DATA) }}">{{ APPLIED_JOB_DATA.jobname }}</a></p>{% endif %}
{% if getLocationText(APPLIED_JOB_DATA.city, APPLIED_JOB_DATA.state, APPLIED_JOB_DATA.country) %}<p><strong>{{ Location_ }}</strong> {{ getLocationText(APPLIED_JOB_DATA.city, APPLIED_JOB_DATA.state, APPLIED_JOB_DATA.country) }}</p>{% endif %}
