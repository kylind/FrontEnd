    "version": "0.2.0",
    "configurations": [{
            "name": "Launch index.html (disable sourcemaps)",
            "type": "chrome",
            "request": "launch",
            "sourceMaps": false,
            "url": "127.0.0.1:3051/index",
            "webRoot": "${workspaceRoot}/easygoing/web/views"

        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${file}"
        }
    ]
    
